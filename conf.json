{
    "opts": {
        "template": "templates/default",
        "encoding": "utf8",
        "destination": "./doc/",
        "recurse": true
    },
    "source": {
        "include": [
            "./js"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    }
}